
    {{#each photos}}

    <div>
        <label for="name">Name: </label>
        <input type="text" name="name" value={{this.name}}>
    </div>
    <div>
        <label for="photoUrl">URL: </label>
        <input type="text" name="photoUrl" value="{{this.photo_url}}" required>
    </div>

    <form action="/{{../userId}}/{{../placesId}}/photos/{{this.id}}?_method=DELETE" method="POST">
    <input type="submit" Value="Delete">
</form>


    {{/each}}
    <form action="/{{userId}}/places/{{placesId}}/show">
        <input type="submit" value="Back">
    </form>
