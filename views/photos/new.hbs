
<form action="/{{userId}}/{{placesId}}/photos/" method="POST">
    <div>
        <label for="name">Name: </label>
        <input type="text" name="name">
    </div>
    <div>
        <label for="photoUrl">URL: </label>
        <input type="url" name="photoUrl"  pattern="https?://.+" required>
    </div>
    <div>
        <input type="submit" value="Add Photo Url">
    </div>
</form>
<form action="/{{userId}}/places/{{placesId}}/show">
    <input type="submit" value="back">
</form>