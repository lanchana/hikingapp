<h1>Loged in</h1>

<ul>
    {{#each user.places}}
    <li>
    <form action="/{{../user.id}}/places/{{this.id}}?_method=Delete" method="POST">
        Hiking Place: {{this.name}}<input type="submit" value="DELETE">

    </form>
    <form action="/{{../user.id}}/places/{{this.id}}/edit"><input type="submit" value="EDIT">
    </form>

    </li>
    <li>log & lat: {{this.longitude}},  {{this.latitude}}</li>
    <li>Description: {{this.description}}</li>
        {{#each this.photos}}

        <ul>
            <li>{{this.photo_url}}</li>
            <li>{{this.name}}</li>

            <form action="/{{../../user.id}}/{{../../places.id}}/photos/{{this.id}}?_method=DELETE" method="POST">
            <input type="submit" Value="Delete Photo">
            </form>
        </ul>
        {{/each}}
    <form action="/{{../user.id}}/{{this.id}}/photos/new">
    <input type="Submit" value="Add Photos">
</form>


    {{/each}}
</ul>

<form action="/{{user.id}}/places/new">
    <input type="Submit" value="Add New">
</form>
